{
  "name": "smartagentalliance-frontend",
  "version": "1.0.0",
  "description": "Smart Agent Alliance - Next.js 15 Frontend for staging.smartagentalliance.com",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:clean": "rm -rf .next && next dev",
    "dev:safe": "bash scripts/cleanup-dev.sh && next dev",
    "dev:check": "lsof -i :3000 && echo '⚠️  Port 3000 in use!' || npm run dev",
    "kill:dev": "pkill -f 'next dev' && pkill -f 'next-server' || echo '✅ No dev servers running'",
    "cleanup": "bash scripts/cleanup-dev.sh",
    "build": "next build",
    "build:static": "echo '⚠️  DEPRECATED: Use export:safe instead (this uses unsafe file movement)' && exit 1",
    "build:static:clean": "echo '⚠️  DEPRECATED: Use export:clean instead (this uses unsafe file movement)' && exit 1",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "export": "npm run generate:css && bash -c 'mv next.config.ts next.config.dynamic.ts && mv next.config.static.ts next.config.ts && next build; EXIT_CODE=$?; mv next.config.ts next.config.static.ts && mv next.config.dynamic.ts next.config.ts; exit $EXIT_CODE'",
    "export:clean": "rm -rf out .next && npm run export",
    "export:safe": "bash scripts/export-static-safe.sh",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:unit": "vitest run tests/",
    "test:e2e": "playwright test tests/e2e",
    "test:e2e:headed": "playwright test tests/e2e --headed",
    "test:e2e:debug": "playwright test tests/e2e --debug",
    "test:e2e:auth": "playwright test tests/e2e/auth",
    "test:e2e:users": "playwright test tests/e2e/users",
    "test:e2e:roles": "playwright test tests/e2e/roles",
    "test:e2e:activity": "playwright test tests/e2e/activity",
    "test:e2e:authorization": "playwright test tests/e2e/authorization",
    "test:e2e:accessibility": "playwright test tests/e2e/accessibility",
    "test:e2e:responsive": "playwright test tests/e2e/responsive",
    "test:e2e:ui": "playwright test tests/e2e --grep-invert '@api'",
    "test:e2e:api": "playwright test tests/e2e --grep '@api'",
    "test:e2e:smoke": "playwright test tests/e2e/auth/login.spec.ts tests/e2e/auth/logout.spec.ts",
    "test:e2e:ci": "playwright test tests/e2e --project=chromium",
    "test:e2e:mobile": "playwright test tests/e2e --project='Mobile Chrome' --project='Mobile Safari'",
    "test:e2e:report": "playwright show-report",
    "test:e2e:notify": "bash /home/claude-flow/scripts/run-e2e-tests-with-notify.sh --env production",
    "test:e2e:notify:staging": "bash /home/claude-flow/scripts/run-e2e-tests-with-notify.sh --env staging",
    "test:suites": "playwright test tests/suites/",
    "test:suites:home": "playwright test tests/suites/home.spec.ts",
    "test:suites:blog": "playwright test tests/suites/blog.spec.ts",
    "test:suites:mc": "playwright test tests/suites/master-controller.spec.ts",
    "test:suites:auth": "playwright test tests/suites/auth-flows.spec.ts",
    "test:suites:mobile": "playwright test tests/suites/mobile-responsive.spec.ts",
    "test:suites:visual": "playwright test tests/suites/visual-regression.spec.ts",
    "test:suites:headed": "playwright test tests/suites/ --headed",
    "test:suites:debug": "playwright test tests/suites/ --debug",
    "test:suites:chromium": "playwright test tests/suites/ --project=chromium",
    "test:suites:firefox": "playwright test tests/suites/ --project=firefox",
    "test:suites:webkit": "playwright test tests/suites/ --project=webkit",
    "test:suites:smoke": "playwright test tests/suites/home.spec.ts tests/suites/auth-flows.spec.ts -g 'should load'",
    "test:suites:ci": "playwright test tests/suites/ --project=chromium --workers=2",
    "test:suites:report": "playwright show-report",
    "test:suites:visual:update": "playwright test tests/suites/visual-regression.spec.ts --update-snapshots",
    "test:suites:all": "npm run test:suites && npm run test:suites:report",
    "test:performance": "jest tests/master-controller/performance",
    "test:a11y": "bash scripts/accessibility-test.sh",
    "test:a11y:playwright": "playwright test tests/e2e/accessibility",
    "test:visual": "playwright test tests/master-controller/visual --update-snapshots",
    "test:all": "npm run type-check && npm run test:unit && npm run test:e2e",
    "build:prod": "bash scripts/production-build.sh",
    "security:scan": "bash scripts/security-scan.sh",
    "security:audit": "npm audit --audit-level=moderate",
    "security:check": "npm run security:audit && npm run type-check && npm run lint",
    "perf:lighthouse": "bash scripts/lighthouse-test.sh",
    "perf:analyze": "npm run build && npx @next/bundle-analyzer",
    "perf:check": "npm run build && npm run perf:lighthouse",
    "page:create": "bash /home/claude-flow/scripts/build-page.sh",
    "page:validate": "tsx /home/claude-flow/scripts/validators/typography-validator.ts",
    "page:validate:verbose": "VERBOSE=true tsx /home/claude-flow/scripts/validators/typography-validator.ts",
    "blog:static:build": "bash scripts/build-static-blog.sh",
    "blog:static:deploy": "bash scripts/deploy-static-blog.sh",
    "blog:static:deploy:preview": "bash scripts/deploy-static-blog.sh --preview",
    "blog:static:quick": "bash scripts/quick-deploy-blog.sh",
    "blog:static:quick:preview": "bash scripts/quick-deploy-blog.sh --preview",
    "blog:static:test": "bash scripts/test-static-blog.sh",
    "generate:css": "tsx scripts/generate-static-css.ts",
    "verify:static-export": "bash scripts/verify-static-export.sh",
    "test:static-animations": "playwright test tests/static-deployment/verify-animations.spec.ts",
    "test:static-mc": "playwright test tests/static-deployment/verify-master-controller.spec.ts",
    "test:static-all": "playwright test tests/static-deployment/",
    "rollback:static": "bash scripts/rollback-static-deployment.sh"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.901.0",
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@react-email/components": "^0.5.5",
    "@react-email/render": "^1.3.1",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.58.0",
    "@tanstack/react-query": "^5.90.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/zxcvbn": "^4.4.5",
    "bcryptjs": "^3.0.2",
    "canvas-confetti": "^1.9.3",
    "class-variance-authority": "^0.7.1",
    "client-only": "^0.0.1",
    "clsx": "^2.1.1",
    "critters": "^0.0.23",
    "crypto": "^1.0.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "framer-motion": "^12.23.22",
    "jose": "^6.1.0",
    "lucide-react": "^0.544.0",
    "next": "^16.0.0",
    "next-themes": "^0.4.6",
    "puppeteer": "^24.23.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-email": "^4.2.12",
    "react-hook-form": "^7.64.0",
    "react-slot-counter": "^3.3.1",
    "resend": "^6.1.2",
    "sonner": "^2.0.7",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "three": "^0.120.0",
    "zod": "^4.1.11",
    "zustand": "^5.0.8",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@axe-core/react": "^4.10.2",
    "@cloudflare/next-on-pages": "^1.13.16",
    "@playwright/test": "^1.55.1",
    "@tailwindcss/postcss": "^4.1.14",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/canvas-confetti": "^1.9.0",
    "@types/node": "^22.10.2",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.2",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.20",
    "axe-core": "^4.10.3",
    "eslint": "^9.17.0",
    "eslint-config-next": "^15.1.3",
    "happy-dom": "^19.0.2",
    "jest-mock-extended": "^4.0.0",
    "pa11y": "^9.0.1",
    "pg": "^8.16.3",
    "playwright": "^1.55.1",
    "postcss": "^8.4.49",
    "postcss-import": "^16.1.1",
    "supabase": "^2.51.0",
    "tailwindcss": "^4.0.0",
    "tsx": "^4.20.6",
    "typescript": "^5.7.2",
    "vitest": "^3.2.4",
    "wrangler": "^4.42.2"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
}
