{
  "name": "@saa/public-site",
  "version": "1.0.0",
  "description": "Public-facing static site for Smart Agent Alliance",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3001",
    "prebuild": "npm run generate:blog-posts",
    "build": "next build",
    "postbuild": "npm run sync-images && npm run generate:sitemap && npm run generate:routes",
    "generate:sitemap": "next-sitemap",
    "generate:routes": "echo '{\"version\":1,\"include\":[\"/*\"],\"exclude\":[\"/fonts/*\",\"/_next/*\",\"/*.ico\",\"/*.txt\",\"/*.xml\",\"/*.json\",\"/*.png\",\"/*.jpg\",\"/*.webp\"]}' > out/_routes.json && chmod 644 out/_routes.json",
    "start": "next start -p 3001",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "export": "npm run build",
    "generate:css": "tsx scripts/generate-static-css.ts",
    "generate:blog-posts": "tsx scripts/generate-blog-posts-json.ts",
    "sync-images": "tsx scripts/sync-cloudflare-images.ts",
    "build:full": "npm run build",
    "test:performance": "node scripts/performance-test.js",
    "diagnostics": "npx tsx scripts/performance-diagnostics.ts",
    "diagnostics:prod": "npx tsx scripts/performance-diagnostics.ts https://saabuildingblocks.com"
  },
  "dependencies": {
    "@imgly/background-removal": "^1.7.0",
    "@saa/shared": "*",
    "@types/react-color": "^3.0.13",
    "canvas-confetti": "^1.9.4",
    "clsx": "^2.1.1",
    "gsap": "^3.14.2",
    "lenis": "^1.3.15",
    "next": "^16.0.0",
    "qr-code-styling": "^1.9.2",
    "react": "^19.2.0",
    "react-color": "^2.19.3",
    "react-dom": "^19.2.0",
    "tailwind-merge": "^2.6.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^16.0.3",
    "@playwright/test": "^1.56.1",
    "@tailwindcss/postcss": "^4.1.14",
    "@types/canvas-confetti": "^1.9.0",
    "@types/node": "^22.10.2",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.2",
    "@vercel/speed-insights": "^1.2.0",
    "autoprefixer": "^10.4.20",
    "babel-plugin-react-compiler": "^1.0.0",
    "dotenv": "^17.2.3",
    "eslint": "^9.17.0",
    "eslint-config-next": "^15.1.3",
    "next-sitemap": "^4.2.3",
    "playwright": "^1.56.1",
    "postcss": "^8.4.49",
    "postcss-import": "^16.1.1",
    "tailwindcss": "^4.1.17",
    "tsx": "^4.19.2",
    "typescript": "^5.9.3",
    "web-vitals": "^5.1.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
}
